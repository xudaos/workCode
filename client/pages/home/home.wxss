/* pages/home/home.wxss */
.home {
  width: 100%;
  height: 100%;
}
.body {
  width: 100%;
  height: 100%;
}
.main {
  width: 100%;
  height: 92%;
  display: flex;
  flex-direction: column;
  border: 1px solid rgb(0,0,0);
}
.row {
  width: 100%;
  flex: 1;
  display: flex;
}
.grid {
  flex: 1;
  height: 100%;
  font-size: 12rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: rgba(0,0,0,0);
}
.bottom {
  width: 100%;
  height: 8%;
  display: flex;
  align-items: center;
  padding: 0 2%;
}
.test-btn {
  background: rgba(0,0,255,0.5);
  color: rgb(255,255,255);
  font-size: 30rpx;
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
}
